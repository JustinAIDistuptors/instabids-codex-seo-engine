{
  "name": "instabids-codex-seo-engine",
  "version": "1.0.0",
  "description": "An advanced, fully autonomous AI system designed to generate, optimize, and maintain thousands of hyper-localized service pages across US ZIP codes for InstaBids.ai",
  "main": "src/backend/server.js",
  "scripts": {
    "start": "node dist/backend/server.js",
    "dev": "nodemon --exec ts-node src/backend/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "db:setup": "ts-node src/backend/scripts/db-setup.ts",
    "db:seed": "ts-node src/backend/scripts/db-seed.ts",
    "generate:types": "ts-node src/backend/scripts/generate-types.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JustinAIDistuptors/instabids-codex-seo-engine.git"
  },
  "keywords": [
    "seo",
    "ai",
    "autonomous",
    "content-generation",
    "multi-agent",
    "codex"
  ],
  "author": "InstaBids Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JustinAIDistuptors/instabids-codex-seo-engine/issues"
  },
  "homepage": "https://github.com/JustinAIDistuptors/instabids-codex-seo-engine#readme",
  "dependencies": {
    "@pinecone-database/pinecone": "^1.2.0",
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "neo4j-driver": "^5.11.0",
    "node-cron": "^3.0.2",
    "openai": "^4.33.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@octokit/rest": "^20.0.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/sequelize": "^4.28.18",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
